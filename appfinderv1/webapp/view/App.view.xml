<mvc:View
    controllerName="com.mindset.accelerator.appfinder.controller.App"
    xmlns:smartchart="sap.ui.comp.smartchart"  xmlns:mvc="sap.ui.core.mvc" xmlns:smartTable="sap.ui.comp.smarttable" xmlns:tnt="sap.tnt"
    displayBlock="true" xmlns:ui="sap.ui.table" xmlns:core="sap.ui.core" xmlns:f="sap.f"
    xmlns="sap.m" xmlns:l="sap.ui.layout" xmlns:form="sap.ui.layout.form" xmlns:u="sap.ui.unified"
     xmlns:viz="sap.viz.ui5.controls"  xmlns:c="sap.suite.ui.commons"
	xmlns:viz.feeds="sap.viz.ui5.controls.common.feeds"  xmlns:viz.data="sap.viz.ui5.data">
    <Shell id="shell">
        <App id="app">
            <pages>
            <Page showHeader="false">
            
                <!-- <f:DynamicPage id="dynamicPageId" fitContent="true" >
              	<f:header>
				<f:DynamicPageHeader pinnable="false">
					<f:content> -->
                        <form:SimpleForm backgroundDesign="Solid" editable="true" layout="ResponsiveGridLayout" labelSpanXL="3" labelSpanL="3" labelSpanM="4"
			                    labelSpanS="4" adjustLabelSpan="false" singleContainerFullSize="false" class="reduceFormTitleHeight">
                            <form:content>
                                <Label design="Bold" text="{i18n>chooseVersion}" />
                                <Input value="{LocalModel>/releaseName} - {LocalModel>/releaseId}" valueHelpOnly="true" showValueHelp="true"  valueHelpRequest="onChooseVersionValueHelpOpen">
                                    <layoutData>
                                        <l:GridData span="XL1 L5 M3 S4" />
                                    </layoutData>
                                </Input>
                                <!-- <ComboBox selectedKey="{LocalModel>/chooseVersion}" items="{path: '/Releases', sorter: [{path: 'releaseRank', descending: 'true'}]}" showSecondaryValues="true">
                                    <core:ListItem text="{releaseName}" key="{releaseName}|{releaseId}" additionalText="{releaseId}" />
                                    <layoutData>
                                        <l:GridData span="XL1 L6 M3 S4" />
                                    </layoutData>
                                </ComboBox> -->
                                <Label design="Bold" text="{i18n>chooseFrom}" />
                                <RadioButtonGroup columns="3" selectedIndex="{LocalModel>/chooseFromOption}" select="onSelectRdBtn">
                                    <buttons>
                                        <RadioButton text="{i18n>manual}"/>
                                        <RadioButton class="sapUiTinyMarginBegin" text="{i18n>upload}"/>
                                         <RadioButton class="sapUiTinyMarginBegin" text="{i18n>sto3upload}"/>
                                    </buttons>
                                </RadioButtonGroup>
                                <Label design="Bold" required="true" text="{i18n>chooseFile}"/>
                                <u:FileUploader value="{LocalModel>/File}" id="idFileUploader" useMultipart="false" sameFilenameAllowed="true" fileType="CSV,csv,xlsx,XLSX" visible="{= ${LocalModel>/chooseFromOption} === 1 || ${LocalModel>/chooseFromOption} === 2 ? true : false}"
                                            icon="sap-icon://upload" sendXHR="true" iconFirst="true" typeMissmatch="onFileTypeMissMatch" style="Emphasized" change="onChangeUpload" uploadComplete="onUploadComplete">
                                </u:FileUploader>
                                <Link text="Download Template" press="onDownloadTemp" visible="{= ${LocalModel>/chooseFromOption} === 1 ? true : false}"/>
                                <Label design="Bold" text="{i18n>TCodes}" required="true" />
                                <TextArea rows="3" liveChange="onChangeTCodesInput" placeholder="{i18n>plachldr}" visible="{= ${LocalModel>/chooseFromOption} === 0 ? true : false}" ></TextArea>
                                <!-- <Button type="Transparent" press="onPressInfoBtn" visible="{= ${LocalModel>/chooseFromOption} === 0 ? true : false}" icon="sap-icon://information">
                                    <layoutData>
                                        <l:GridData span="XL1 L1 M3 S4" />
                                    </layoutData>
                                </Button> -->
                                <core:Icon class="sapUiTinyMarginTop" press="onPressInfoBtn" tooltip="Information" src="sap-icon://information" color="#0854A0" visible="{= ${LocalModel>/chooseFromOption} === 0 ? true : false}">
                                    <core:layoutData>
                                        <l:GridData span="XL1 L1 M3 S4" />
                                    </core:layoutData>
                                </core:Icon>
                                <Label />
                                <Button type="Emphasized" text="{i18n>submit}" press="onPressSubmit" enabled="{= ${LocalModel>/chooseFromOption} === 0 ? ${LocalModel>/PastedTcodes} ? true : false : ${LocalModel>/File} ? true : false}">
                                    <layoutData>
                                        <l:GridData span="XL2 L2 M4 S6" />
                                    </layoutData>
                                </Button>
                            </form:content>
                        </form:SimpleForm>
                        <!-- </f:content>
				</f:DynamicPageHeader>
			</f:header>
            <f:content> -->
            	<Toolbar class="sapUiTinyMarginTop">
				<ToolbarSpacer/>
			<SegmentedButton selectedKey="{LocalModel>/displaySelect}" selectionChange="onSelectSegemntBtn">
				<items>
					<SegmentedButtonItem icon="sap-icon://table-view" key="Table"/>
					<SegmentedButtonItem icon="sap-icon://bar-chart" key="Chart"/>
					
				</items>
			</SegmentedButton>
			</Toolbar>
                <smartTable:SmartTable id="smartTable" class="sapUiSmallMargin" busyIndicatorDelay="0" entitySet="InputFilterParam" tableBindingPath="LocalModel>/Apps" tableType="Table" showTablePersonalisation="true" useVariantManagement="false"
						 header="Applications" beforeExport="onBeforeExport" ignoredFields="InpFilterValue" initialNoDataText="{i18n>initNoData}" persistencyKey="smartItemsTable" enableAutoBinding="false"
						showRowCount="true" showFullScreenButton="true" beforeRebindTable="onBeforeRebindTable" useExportToExcel="false" visible="{= ${LocalModel>/displaySelect} === 'Table' ? true: false}">
                        <smartTable:noData>
                            <Text class="sapUiLargeMarginTop" text="{i18n>noData}"/>
                        </smartTable:noData>
                        <smartTable:customToolbar>
                            <OverflowToolbar>
                                <ToolbarSpacer />
                                <GenericTag text="{i18n>totalTCodes}" design="StatusIconHidden" status="Information">
			                        <ObjectNumber state="Information" emphasized="false" number="{LocalModel>/TCodesCount}" />
		                        </GenericTag>
                                <Button icon="sap-icon://excel-attachment" text="{i18n>export}" type="Transparent" tooltip="{i18n>expTltp}" press="onCustomExcelExport" enabled="{LocalModel>/ExporBtnEnable}"/>
                            </OverflowToolbar>
                        </smartTable:customToolbar>
                        <ui:Table selectionMode="None" fixedColumnCount="3" visibleRowCountMode="Auto">
							<ui:columns>
                                <ui:Column width="6em" filterProperty="TCode" sortProperty="TCode">
									<ui:customData>
										<core:CustomData key="p13nData"
											value='\{"columnKey": "TCode", "columnIndex":"0", "filterProperty": "TCode", "sortProperty": "TCode", "type":"string", "leadingProperty": "TCode"}'/>
									</ui:customData>
									<Label design="Bold" text="{i18n>tCode}"/>
									<ui:template>
										<Text text="{LocalModel>TCode}"/>
									</ui:template>
                                </ui:Column>
								<ui:Column width="6em" filterProperty="appId" sortProperty="appId">
									<ui:customData>
										<core:CustomData key="p13nData"
											value='\{"columnKey": "appId", "columnIndex":"1", "filterProperty": "appId", "sortProperty": "appId", "type":"string", "leadingProperty": "appId"}'/>
									</ui:customData>
									<Label design="Bold" text="{i18n>appID}"/>
									<ui:template>
										<Text text="{LocalModel>appId}"/>
									</ui:template>
                                </ui:Column>
                                <ui:Column width="20em" hAlign="Begin" filterProperty="AppName" sortProperty="AppName">
									<ui:customData>
										<core:CustomData key="p13nData"
											value='\{"columnKey": "AppName", "columnIndex":"2", "filterProperty": "AppName", "sortProperty": "AppName", "type":"string", "leadingProperty": "AppName"}'/>
									</ui:customData>
									<Label design="Bold" text="{i18n>appName}"/>
									<ui:template>
                                        <Link target="_blank" href="{LocalModel>AppLink}" text="{LocalModel>AppName}" tooltip="{LocalModel>AppLink}" />
									</ui:template>
                                </ui:Column>

                                <ui:Column width="15em" hAlign="Center" filterProperty="ApplicationType" sortProperty="ApplicationType">
									<ui:customData>
										<core:CustomData key="p13nData"
											value='\{"columnKey": "ApplicationType", "columnIndex":"3", "filterProperty": "ApplicationType", "sortProperty": "ApplicationType", "type":"string", "leadingProperty": "ApplicationType"}'/>
									</ui:customData>
									<Label design="Bold" text="{i18n>appType}"/>
									<ui:template>
										<Text text="{LocalModel>ApplicationType}"/>
									</ui:template>
                                </ui:Column>
                                <ui:Column width="12em" filterProperty="RoleName" sortProperty="RoleName">
									<ui:customData>
										<core:CustomData key="p13nData"
											value='\{"columnKey": "RoleName", "columnIndex":"4", "filterProperty": "RoleName", "sortProperty": "RoleName", "type":"string", "leadingProperty": "RoleName"}'/>
									</ui:customData>
									<Label design="Bold" text="{i18n>role}"/>
									<ui:template>
										<Text text="{LocalModel>RoleName}"/>
									</ui:template>
                                </ui:Column>
                                <ui:Column width="6em" hAlign="Center" filterProperty="Lighthouse" >
									<ui:customData>
										<core:CustomData key="p13nData"
											value='\{"columnKey": "Lighthouse", "columnIndex":"5", "filterProperty": "Lighthouse", "type":"boolean", "leadingProperty": "Lighthouse"}'/>
									</ui:customData>
									<Label design="Bold" text="{i18n>lighthouse}"/>
									<ui:template>
										<core:Icon color="{= ${LocalModel>Lighthouse} ? '#168282':'#C14646'}" size="1.4em" src="sap-icon://BusinessSuiteInAppSymbols/icon-lighthouse" tooltip="{= ${LocalModel>Lighthouse} ? 'Yes':'No'}"/>
									</ui:template>
                                </ui:Column>
                                <ui:Column width="23em" hAlign="Center" filterProperty="GTMLoBName" sortProperty="GTMLoBName">
									<ui:customData>
										<core:CustomData key="p13nData"
											value='\{"columnKey": "GTMLoBName", "columnIndex":"6", "filterProperty": "GTMLoBName", "sortProperty": "GTMLoBName", "type":"string", "leadingProperty": "GTMLoBName"}'/>
									</ui:customData>
									<Label design="Bold" text="{i18n>funcArea}"/>
									<ui:template>
                                        <l:HorizontalLayout class="sapUiTinyMarginTop" allowWrapping="true" content="{LocalModel>LoB}">
                                            <tnt:InfoLabel class="sapUiTinyMarginBegin" text="{LocalModel>FA}" colorScheme="7"/>
                                        </l:HorizontalLayout>
									</ui:template>
                                </ui:Column>
                                <ui:Column width="15em" visible="false" filterProperty="BusinessRole" sortProperty="BusinessRole">
									<ui:customData>
										<core:CustomData key="p13nData"
											value='\{"columnKey": "BusinessRole", "columnIndex":"7", "filterProperty": "BusinessRole", "sortProperty": "BusinessRole", "type":"string", "leadingProperty": "BusinessRole"}'/>
									</ui:customData>
									<Label design="Bold" text="{i18n>businessRole}"/>
									<ui:template>
										<Text text="{LocalModel>BusinessRole}"/>
									</ui:template>
                                </ui:Column>
                                <ui:Column width="9em" hAlign="Center" filterProperty="FormFactors" sortProperty="FormFactors">
									<ui:customData>
										<core:CustomData key="p13nData"
											value='\{"columnKey": "FormFactors", "columnIndex":"8", "filterProperty": "FormFactors", "sortProperty": "FormFactors", "type":"string", "leadingProperty": "FormFactors"}'/>
									</ui:customData>
									<Label design="Bold" text="{i18n>deviceTypes}"/>
									<ui:template>
                                        <FlexBox alignItems="Center" justifyContent="SpaceAround">
                                            <core:Icon src="sap-icon://laptop" tooltip="Desktop" color="{= ${LocalModel>FormFactors}.indexOf('Desktop') !== -1 ? '#168282':'#C14646'}" />
                                            <core:Icon src="sap-icon://iphone" tooltip="Smartphone" color="{= ${LocalModel>FormFactors}.indexOf('Smartphone') !== -1 ? '#168282':'#C14646'}"/>
                                            <core:Icon src="sap-icon://ipad" tooltip="Tablet" color="{= ${LocalModel>FormFactors}.indexOf('Tablet') !== -1 ? '#168282':'#C14646'}"/>
                                        </FlexBox>
										<!-- <Text text="{LocalModel>FormFactors}"/> -->
									</ui:template>
                                </ui:Column>
                                <ui:Column width="15em" filterProperty="UITechnology" sortProperty="UITechnology">
									<ui:customData>
										<core:CustomData key="p13nData"
											value='\{"columnKey": "UITechnology", "columnIndex":"9", "filterProperty": "UITechnology", "sortProperty": "UITechnology", "type":"string", "leadingProperty": "UITechnology"}'/>
									</ui:customData>
									<Label design="Bold" text="{i18n>uiTech}"/>
									<ui:template>
										<Text text="{LocalModel>UITechnology}"/>
									</ui:template>
                                </ui:Column>
                                <ui:Column width="15em" visible="false" filterProperty="BSPName" sortProperty="BSPName">
									<ui:customData>
										<core:CustomData key="p13nData"
											value='\{"columnKey": "BSPName", "columnIndex":"10", "filterProperty": "BSPName", "sortProperty": "BSPName", "type":"string", "leadingProperty": "BSPName"}'/>
									</ui:customData>
									<Label design="Bold" text="{i18n>ui5TechName}"/>
									<ui:template>
										<Text text="{LocalModel>BSPName}"/>
									</ui:template>
                                </ui:Column>
                                <ui:Column width="20em" visible="false" filterProperty="oDataSrv" sortProperty="oDataSrv">
									<ui:customData>
										<core:CustomData key="p13nData"
											value='\{"columnKey": "oDataSrv", "columnIndex":"11", "filterProperty": "oDataSrv", "sortProperty": "oDataSrv", "type":"string", "leadingProperty": "oDataSrv"}'/>
									</ui:customData>
									<Label design="Bold" text="{i18n>oDataSrvs}"/>
									<ui:template>
										<Text text="{LocalModel>oDataSrv}"/>
									</ui:template>
                                </ui:Column>
                                <ui:Column width="20em" visible="false" filterProperty="TargetMapping" sortProperty="TargetMapping">
									<ui:customData>
										<core:CustomData key="p13nData"
											value='\{"columnKey": "TargetMapping", "columnIndex":"12", "filterProperty": "TargetMapping", "sortProperty": "TargetMapping", "type":"string", "leadingProperty": "TargetMapping"}'/>
									</ui:customData>
									<Label design="Bold" text="{i18n>targetMapping}"/>
									<ui:template>
										<Text text="{LocalModel>TargetMapping}"/>
									</ui:template>
                                </ui:Column>
                                <ui:Column width="20em" visible="false" filterProperty="TechnicalCatalog" sortProperty="TechnicalCatalog">
									<ui:customData>
										<core:CustomData key="p13nData"
											value='\{"columnKey": "TechnicalCatalog", "columnIndex":"13", "filterProperty": "TechnicalCatalog", "sortProperty": "TechnicalCatalog", "type":"string", "leadingProperty": "TechnicalCatalog"}'/>
									</ui:customData>
									<Label design="Bold" text="{i18n>technicalCatalog}"/>
									<ui:template>
										<Text text="{LocalModel>TechnicalCatalog}"/>
									</ui:template>
                                </ui:Column>
                                <ui:Column width="20em" visible="false" filterProperty="BusinessCatalog" sortProperty="BusinessCatalog">
									<ui:customData>
										<core:CustomData key="p13nData"
											value='\{"columnKey": "BusinessCatalog", "columnIndex":"14", "filterProperty": "BusinessCatalog", "sortProperty": "BusinessCatalog", "type":"string", "leadingProperty": "BusinessCatalog"}'/>
									</ui:customData>
									<Label design="Bold" text="{i18n>businessCatalog}"/>
									<ui:template>
										<Text text="{LocalModel>BusinessCatalog}"/>
									</ui:template>
                                </ui:Column>
                                <ui:Column width="20em" visible="false" filterProperty="BusinessGroup" sortProperty="BusinessGroup">
									<ui:customData>
										<core:CustomData key="p13nData"
											value='\{"columnKey": "BusinessGroup", "columnIndex":"15", "filterProperty": "BusinessGroup", "sortProperty": "BusinessGroup", "type":"string", "leadingProperty": "BusinessGroup"}'/>
									</ui:customData>
									<Label design="Bold" text="{i18n>businessGroup}"/>
									<ui:template>
										<Text text="{LocalModel>BusinessGroup}"/>
									</ui:template>
                                </ui:Column>
                                <!-- <ui:Column hAlign="Center" width="12em">
									<ui:customData>
										<core:CustomData key="p13nData"
											value='\{"columnKey": "AppLink", "columnIndex":"9", "type":"string", "leadingProperty": "AppLink"}'/>
									</ui:customData>
									<Label design="Bold" text="{i18n>appLink}"/>
									<ui:template>
                                        <core:Icon src="sap-icon://action" press="onPressLink" tooltip="{LocalModel>AppLink}" color="#0715e3" ></core:Icon>
									</ui:template>
                                </ui:Column> -->
							</ui:columns>
						</ui:Table>
                    </smartTable:SmartTable>
                    <VBox justifyContent="Center" visible="{= ${LocalModel>/displaySelect} === 'Chart' ? true: false}" class="sapUiMediumMarginBegin">
                     <l:VerticalLayout id="containerLayout" width="100%" >
                                <l:BlockLayout id="BlockLayout" background="Dashboard">
                                <l:BlockLayoutRow  >
                                    <l:BlockLayoutCell   > 
                                    <c:ChartContainer id="chartContainer1" showFullScreen="true">
                                            <c:content>
                                                <c:ChartContainerContent>
                                                    <c:content>
                                       
                                        <viz:VizFrame id="idVizFrame2"  uiConfig="{applicationSet:'fiori'}"  height="500px" width="100%" vizType="bar" vizProperties="{plotArea: { drawingEffect: 'glossy'}, title:{ text:'By Functional Area',visible:true},dataLabel:{visible:true,showTotal:true},valueAxis:{ title:{text:'App Count'}}}"> 
                                            <viz:dataset>
                                                <viz.data:FlattenedDataset data="{LocalModel>/aChartDataByFunArea}">
                                                    <viz.data:dimensions>
                                                        <viz.data:DimensionDefinition name="Functional Area" value="{LocalModel>Functional Area}"/>
                                                    </viz.data:dimensions>
                                                    <viz.data:measures>
                                                        <viz.data:MeasureDefinition name="App Count" value="{LocalModel>App Count}"/>
                                                    </viz.data:measures>
                                                </viz.data:FlattenedDataset>
                                            </viz:dataset>
                                            <viz:feeds>
                                                <viz.feeds:FeedItem id="valueAxisFeed2"  uid="valueAxis" type="Measure" values="App Count"/>
                                                <viz.feeds:FeedItem id="categoryAxisFeed2" uid="categoryAxis" type="Dimension" values="Functional Area"/>
                                            </viz:feeds>
                                        </viz:VizFrame>
                                                     </c:content>
                                                </c:ChartContainerContent>
                                            </c:content>
                                        </c:ChartContainer>
                      	            </l:BlockLayoutCell> 
                                      <l:BlockLayoutCell    > 
                                        <c:ChartContainer id="chartContainer2" showFullScreen="true">
                                            <c:content>
                                                <c:ChartContainerContent>
                                                    <c:content>
                                       
                                        <viz:VizFrame id="idVizFrame3"  uiConfig="{applicationSet:'fiori'}"  height="500px" width="100%" vizType="column" vizProperties="{plotArea: { drawingEffect: 'glossy', colorPalette: ['brown']}, title:{ text:'By Application Type',visible:true},dataLabel:{visible:true,showTotal:true},valueAxis:{ title:{text:'App Count'}}}"> 
                                            <viz:dataset>
                                                <viz.data:FlattenedDataset data="{LocalModel>/aChartDataByAppType}">
                                                    <viz.data:dimensions>
                                                        <viz.data:DimensionDefinition name="Application Type" value="{LocalModel>Application Type}"/>
                                                    </viz.data:dimensions>
                                                    <viz.data:measures>
                                                        <viz.data:MeasureDefinition name="App Count" value="{LocalModel>App Count}"/>
                                                    </viz.data:measures>
                                                </viz.data:FlattenedDataset>
                                            </viz:dataset>
                                            <viz:feeds>
                                                <viz.feeds:FeedItem id="valueAxisFeed3"  uid="valueAxis" type="Measure" values="App Count"/>
                                                <viz.feeds:FeedItem id="categoryAxisFeed3" uid="categoryAxis" type="Dimension" values="Application Type"/>
                                            </viz:feeds>
                                        </viz:VizFrame>
                                             </c:content>
                                                </c:ChartContainerContent>
                                            </c:content>
                                        </c:ChartContainer>
                      	            </l:BlockLayoutCell> 

                                	
                                </l:BlockLayoutRow >
                                 <l:BlockLayoutRow  >
                                    <l:BlockLayoutCell    > 
                                        <c:ChartContainer id="chartContainer3" showFullScreen="true">
                                            <c:content>
                                                <c:ChartContainerContent>
                                                    <c:content>
                                        <viz:VizFrame id="idVizFrame4"  uiConfig="{applicationSet:'fiori'}"  height="500px" width="100%" vizType="pie"  vizProperties="{plotArea: { drawingEffect: 'glossy'}, title:{ text:'By UI Technology '},dataLabel:{visible:true,showTotal:false,type : 'value'}}" > 
                                            <viz:dataset>
                                                <viz.data:FlattenedDataset data="{LocalModel>/aChartDataByUITech}">
                                                    <viz.data:dimensions>
                                                        <viz.data:DimensionDefinition name="UITechnology" value="{LocalModel>UITechnology}"/>
                                                    </viz.data:dimensions>
                                                    <viz.data:measures>
                                                        <viz.data:MeasureDefinition name="App Count" value="{LocalModel>App Count}"/>
                                                    </viz.data:measures>
                                                </viz.data:FlattenedDataset>
                                            </viz:dataset>
                                            <viz:feeds>
                                                <viz.feeds:FeedItem id="valueAxisFeed4"  uid="size" type="Measure" values="App Count"/>
                                                <viz.feeds:FeedItem id="categoryAxisFeed4" uid="color" type="Dimension" values="UITechnology"/>
                                            </viz:feeds>
                                        </viz:VizFrame>
                                        </c:content>
                                                </c:ChartContainerContent>
                                            </c:content>
                                        </c:ChartContainer>
                      	            </l:BlockLayoutCell> 
                                      <l:BlockLayoutCell   > 
                                        <c:ChartContainer id="chartContainer4" showFullScreen="true">
                                            <c:content>
                                                <c:ChartContainerContent>
                                                    <c:content>
                                        <viz:VizFrame id="idVizFrame5"  uiConfig="{applicationSet:'fiori'}" height='500px' width="100%"  vizType="donut" vizProperties="{plotArea: { drawingEffect: 'glossy'}, title:{ text:'By Job Role '},dataLabel:{visible:true,showTotal:false,type : 'value'}}" > 
                                            <viz:dataset>
                                                <viz.data:FlattenedDataset data="{LocalModel>/aChartDataByJobRole}">
                                                    <viz.data:dimensions>
                                                        <viz.data:DimensionDefinition name="Job Role" value="{LocalModel>Job Role}"/>
                                                    </viz.data:dimensions>
                                                    <viz.data:measures>
                                                        <viz.data:MeasureDefinition name="App Count" value="{LocalModel>App Count}"/>
                                                    </viz.data:measures>
                                                </viz.data:FlattenedDataset>
                                            </viz:dataset>
                                            <viz:feeds>
                                                <viz.feeds:FeedItem id="valueAxisFeed5"  uid="size" type="Measure" values="App Count"/>
                                                <viz.feeds:FeedItem id="categoryAxisFeed5" uid="color" type="Dimension" values="Job Role"/>
                                            </viz:feeds>
                                        </viz:VizFrame>
                                             </c:content>
                                                </c:ChartContainerContent>
                                            </c:content>
                                        </c:ChartContainer>
                      	            </l:BlockLayoutCell> 

                                	
                                </l:BlockLayoutRow >
                                </l:BlockLayout>
                            </l:VerticalLayout>
                    </VBox>
					
                                    
                      
                       
                        
                        <!-- </f:content>
              </f:DynamicPage> -->
              </Page>
            </pages>
        </App>
    </Shell>
</mvc:View>
